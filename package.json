{
  "name": "lumiq",
  "version": "1.0.0",
  "description": "Lumiq Full Stack Application",
  "private": true,
  "engines": {
    "node": ">=22.19.0",
    "npm": ">=10.0.0"
  },
  "volta": {
    "node": "22.19.0"
  },
  "scripts": {
    "dev:frontend": "cd frontend && npm run dev",

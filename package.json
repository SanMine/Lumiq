    "postinstall": "echo 'Node.js version:' && node --version && echo 'Required: >=22.19.0'"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "devDependencies": {},
  "keywords": [
    "lumiq",
    "react",
    "node",
    "fullstack"
  ],
